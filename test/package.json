{
  "name": "docker-cicd-node-test",
  "version": "1.0.0",
  "description": "Testing for the container",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "ci:e2e:chrome": "cypress run --browser=chrome",
    "ci:e2e:firefox": "cypress run --browser=firefox",
    "ci:e2e:electron": "cypress run",
    "ci:e2e": "npm-run-all -l -p ci:e2e:**",
    "ci:test": "jest --runInBand --detectOpenHandles ./mongo",
    "test": "npm-run-all -l -p ci:**"
  },
  "author": "Kayla Altepeter",
  "license": "MIT",
  "dependencies": {
    "mongoose": "^7.1.0"
  },
  "devDependencies": {
    "cypress": "^12.11.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "8.11",
    "npm-run-all": "^4.1.5"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
